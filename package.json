{
    "name": "laravel-react-starter",
    "version": "0.0.0.1",
    "private": true,
    "scripts": {
        "dev": "npm run development",
        "development": "mix",
        "watchmix": "mix watch",
        "watch-poll": "mix watch -- --watch-options-poll=1000",
        "hotmix": "mix watch --hot",
        "prod": "npm run production",
        
        "productionmix": "mix --production",
        "developmentmix": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress  --config=node_modules/laravel-mix/setup/webpack.config.js",
        "watch": "npm run development -- --watch",
        "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
        "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
    },
    "dependencies": {
        "@babel/preset-react": "^7.0.0",
        "@craco/craco": "^5.6.4",
        "antd": "^4.6.1",
        "axios": "^0.21",
        "bootstrap": "^4.0.0",
        "cross-env": "^5.2.1",
        "date-fns": "^2.0.0-beta.5",
        "date-fns-tz": "^1.0.7",
        "express": "^4.17.1",
        "history": "^4.10.1",
        "immer": "^4.0.0",
        "laravel-mix": "^6.0.6",
        "lodash": "^4.17.19",
        "path": "^0.12.7",
        "polished": "^3.4.1",
        "prop-types": "^15.7.2",
        "react": "^16.9.0",
        "react-css-theme-switcher": "^0.1.6",
        "react-dom": "^16.9.0",
        "react-icons": "^3.7.0",
        "react-perfect-scrollbar": "^1.5.3",
        "react-redux": "^7.1.1",
        "react-router-dom": "^5.1.0",
        "react-scripts": "3.1.2",
        "react-toastify": "^5.4.0",
        "reactotron-react-js": "^3.3.3",
        "reactotron-redux": "^3.1.2",
        "reactotron-redux-saga": "^4.2.2",
        "redux": "^4.0.4",
        "redux-persist": "^6.0.0",
        "redux-saga": "^1.1.1",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.15.2",
        "sass-loader": "^7.1.0",
        "styled-components": "^4.4.0"
    },
    "devDependencies": {
        "autoprefixer": "^9.8.6",
        "axios": "^0.21",
        "babel-jest": "^24.9.0",
        "babel-loader": "8.0.6",
        "babel-plugin-root-import": "^6.4.1",
        "browser-sync": "^2.26.12",
        "craco-antd": "^1.18.1",
        "craco-preact": "^1.1.0",
        "customize-cra": "^0.7.0",
        "eslint": "^6.4.0",
        "eslint-config-airbnb-base": "^14.0.0",
        "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
        "eslint-plugin-import": "^2.18.2",
        "gulp": "^4.0.2",
        "gulp-csso": "^4.0.1",
        "gulp-debug": "^4.0.0",
        "gulp-less": "^4.0.1",
        "gulp-postcss": "^8.0.0",
        "laravel-mix": "^6.0.6",
        "less-plugin-npm-import": "^2.1.0",
        "lodash": "^4.17.19",
        "postcss": "^8.1.14",
        "react-app-rewired": "^2.1.3",
        "webpack": "^5.9.0",
        "webpack-cli": "^4.3.1"
    }
